{
    "workbench.colorTheme": "Default Dark+",
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<C-j>"
            ],
            "commands": [
                ":nohl",
            ]
        },
        {
            "before": [
                "<C-k>",
                "<C-f>"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        }
    ],
    "vim.commandLineModeKeyBindings": [
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ]
}